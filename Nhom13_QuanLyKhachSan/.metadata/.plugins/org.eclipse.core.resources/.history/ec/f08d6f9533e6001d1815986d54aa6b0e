package entity;

/**
 *
 * @author ADMIN
 */
public class HoaDon {
	int maHoaDon;
    ThuePhong datPhong;
    double TienPhong, TienDV, ThanhTien,SoNgayThue=0, SoGioThue=0,PhuThuCheckin=0,PhuThuCheckout=0;

    String TenUser, TenKH,NgayThanhToan,ghiChu ;
    Phong phong;
	public HoaDon(int maHoaDon, ThuePhong datPhong, double tienPhong, double tienDV,
			double thanhTien, double soNgayThue, double soGioThue,
			double phuThuCheckin, double phuThuCheckout, String tenUser,
			String tenKH, String ngayThanhToan, String ghiChu, Phong phong) {
		
		this.maHoaDon = maHoaDon;
		this.datPhong= datPhong;
		TienPhong = tienPhong;
		TienDV = tienDV;
		ThanhTien = thanhTien;
		SoNgayThue = soNgayThue;
		SoGioThue = soGioThue;
		PhuThuCheckin = phuThuCheckin;
		PhuThuCheckout = phuThuCheckout;
		TenUser = tenUser;
		TenKH = tenKH;
		NgayThanhToan = ngayThanhToan;
		this.ghiChu = ghiChu;
		this.phong = phong;
	}
	public HoaDon() {
		super();
	}
	public HoaDon(int maHoaDon) {
		super();
		this.maHoaDon = maHoaDon;
	}
	public int getMaHoaDon() {
		return maHoaDon;
	}
	public void setMaHoaDon(int maHoaDon) {
		this.maHoaDon = maHoaDon;
	}
	
	public ThuePhong getDatPhong() {
		return datPhong;
	}
	public void setDatPhong(ThuePhong datPhong) {
		this.datPhong = datPhong;
	}
	public double getTienPhong() {
		return TienPhong;
	}
	public void setTienPhong(double tienPhong) {
		TienPhong = tienPhong;
	}
	public double getTienDV() {
		return TienDV;
	}
	public void setTienDV(double tienDV) {
		TienDV = tienDV;
	}
	public double getThanhTien() {
		return ThanhTien;
	}
	public void setThanhTien(double thanhTien) {
		ThanhTien = thanhTien;
	}
	public double getSoNgayThue() {
		return SoNgayThue;
	}
	public void setSoNgayThue(double soNgayThue) {
		SoNgayThue = soNgayThue;
	}
	public double getSoGioThue() {
		return SoGioThue;
	}
	public void setSoGioThue(double soGioThue) {
		SoGioThue = soGioThue;
	}
	public double getPhuThuCheckin() {
		return PhuThuCheckin;
	}
	public void setPhuThuCheckin(double phuThuCheckin) {
		PhuThuCheckin = phuThuCheckin;
	}
	public double getPhuThuCheckout() {
		return PhuThuCheckout;
	}
	public void setPhuThuCheckout(double phuThuCheckout) {
		PhuThuCheckout = phuThuCheckout;
	}
	public String getTenUser() {
		return TenUser;
	}
	public void setTenUser(String tenUser) {
		TenUser = tenUser;
	}
	public String getTenKH() {
		return TenKH;
	}
	public void setTenKH(String tenKH) {
		TenKH = tenKH;
	}
	public String getNgayThanhToan() {
		return NgayThanhToan;
	}
	public void setNgayThanhToan(String ngayThanhToan) {
		NgayThanhToan = ngayThanhToan;
	}
	public String getGhiChu() {
		return ghiChu;
	}
	public void setGhiChu(String ghiChu) {
		this.ghiChu = ghiChu;
	}
	public Phong getPhong() {
		return phong;
	}
	public void setPhong(Phong phong) {
		this.phong = phong;
	}
	@Override
	public String toString() {
		return "HoaDon [maHoaDon=" + maHoaDon + ", MaDP=" + datPhong
				+ ", TienPhong=" + TienPhong + ", TienDV=" + TienDV
				+ ", ThanhTien=" + ThanhTien + ", SoNgayThue=" + SoNgayThue
				+ ", SoGioThue=" + SoGioThue + ", PhuThuCheckin="
				+ PhuThuCheckin + ", PhuThuCheckout=" + PhuThuCheckout
				+ ", TenUser=" + TenUser + ", TenKH=" + TenKH
				+ ", NgayThanhToan=" + NgayThanhToan + ", ghiChu=" + ghiChu
				+ ", phong=" + phong + "]";
	}

    
    
    
